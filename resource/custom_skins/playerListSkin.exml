		<e:Label fontFamily="微软雅黑" text="{data.nickName}" size="40"  verticalAlign="middle" left="30%" textColor="0x052305" anchorOffsetX="0"  anchorOffsetY="0"  verticalCenter="0"/>
		<e:Label fontFamily="微软雅黑" text="{data.score}" textAlign="left" size="40" left="50%" verticalAlign="middle" textColor="0xff0000" anchorOffsetY="0"  anchorOffsetX="0" x="0" verticalCenter="0"/>
		<e:Button id="pkBtn"   width="150" height="60" right="10%" skinName="resource/custom_skins/buttonSkin.exml" label="PK"  verticalCenter="0"/>
	</e:Group>
